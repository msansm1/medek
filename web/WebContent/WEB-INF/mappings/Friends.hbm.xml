<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="bzh.msansm1.medek.persistence.model">
	<class name="Friends" table="med_friends">
		<composite-id>
			<key-property name="user"></key-property>
			<key-property name="friend"></key-property>
		</composite-id>
		<property name="isAccepted" type="boolean" />
	</class>
	    
</hibernate-mapping> 